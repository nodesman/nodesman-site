<div class="content-header">
  <div class="header-section">
    <h1>
      <i class="gi gi-notes_2"></i>Contact Form<br><small>Entires of contact form submissions</small>
    </h1>
  </div>
</div>

  <div class="block">
    <div class="block-title">
      <h2><strong>Entries</strong></h2>
    </div>

    <div class="table-reponsive">
      <table id="general-table" class="table table-striped table-vcenter">
        <thead>
        <tr>
          <th>Subject</th>
          <th>Time</th>
          <th>IP Address</th>
          <th></th>
        </tr>
        </thead>
        <tbody>
        <% @contact_entries.each do |entry| %>
        <tr>
          <td>
            <%= link_to "/admin/contact_entry/1" do %>
            <%= entry.subject %>
            <% end %>
          </td>
          <td>
            <%= l entry.created_at, format: :short %>
          </td>
          <td>
            <%= entry.ip_address %>
          </td>
        </tr>
        <% end %>
        </tbody>

      </table>
    </div>
  </div>

</div>